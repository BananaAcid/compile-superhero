{
	"name": "1000phone",
	"manifest_version": 2,
	"version": "6.72",
	"description": "这是千锋一个自动测评的插件😁",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"icons": {
		"16": "public/icons/16.png",
		"48": "public/icons/48.png",
		"128": "public/icons/128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"http://stu.1000phone.net/*"
			],
			"js": [
				"dist/bundle.js"
			]
		}
	],
	"browser_action": {
		"default_title": "",
		"default_icon": {
			"16": "public/icons/16.png",
			"48": "public/icons/48.png",
			"128": "public/icons/128.png"
		},
		"default_popup": "public/template/popup.html"
	}
}