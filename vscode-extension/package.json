{
  "name": "qf",
  "displayName": "Compile Hero",
  "description": "Easy to compile ts, tsx, scss, less, jade and es6 on save without using a build task.",
  "author": "Eno Yao",
  "publisher": "Wscats",
  "version": "6.8.3",
  "icon": "logos/hero2.png",
  "homepage": "https://github.com/Wscats/compile-hero",
  "engines": {
    "vscode": "^1.39.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wscats/compile-hero"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Snippets",
    "Linters",
    "Debuggers",
    "Formatters"
  ],
  "keywords": [
    "1000phone",
    "千锋",
    "千锋互联",
    "千锋教育",
    "mobiletrain",
    "老谢",
    "老姚",
    "琳姐",
    "文婧",
    "测评",
    "自动测评",
    "qf",
    "qianfeng",
    "千峰",
    "前锋",
    "sass",
    "scss",
    "css",
    "typescript",
    "ts",
    "less",
    "ES6",
    "ES5",
    "JS",
    "javascript",
    "compile",
    "translate",
    "tsx",
    "jade",
    "hero",
    "编译",
    "close",
    "port"
  ],
  "activationEvents": [
    "*",
    "onCommand:extension.helloWorld",
    "onCommand:extension.openInBrowser",
    "onCommand:extension.closePort"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.openInBrowser",
        "title": "Open In Browser"
      },
      {
        "command": "extension.closePort",
        "title": "Close Port"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == html",
          "command": "extension.openInBrowser",
          "group": "open-in-browser"
        },
        {
          "when": "resourceLangId == html",
          "command": "extension.closePort",
          "group": "close-port"
        }
      ],
      "editor/context": [
        {
          "when": "resourceLangId == html",
          "command": "extension.openInBrowser",
          "group": "open-in-browser"
        },
        {
          "when": "resourceLangId == html",
          "command": "extension.closePort",
          "group": "close-port"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceLangId == html",
          "command": "extension.openInBrowser",
          "group": "open-in-browser"
        },
        {
          "when": "resourceLangId == html",
          "command": "extension.closePort",
          "group": "close-port"
        }
      ]
    }
  },
  "scripts": {
    "build": "vsce package",
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-jade": "^1.1.0",
    "gulp-less": "^4.0.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.4.0",
    "gulp-typescript": "^5.0.1",
    "gulp-uglify": "^3.0.2",
    "open": "^6.4.0",
    "typescript": "^3.3.1"
  },
  "devDependencies": {
    "vscode": "^1.1.28"
  }
}